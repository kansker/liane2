<?xml version="1.0" encoding="UTF-8"?>
<root>
	<dao name="qa_stylecategory">
		<statement><![CDATA[select * from stylecategory where 1=1]]></statement>
		<parameter condition="status = ?" kind="2" type="int">status</parameter>
		<parameter condition="name like ?" kind="2" type="LikeString">name</parameter>
		<parameter condition="forwho = ?" kind="2" type="int">forwho</parameter>
	</dao>
	<dao name="del_stylecategory">
		<statement><![CDATA[delete from stylecategory where seq = ?]]></statement>
		<parameter type="long">seq</parameter>
	</dao>
	<dao name="qa_photostyle">
		<statement><![CDATA[
		select a.*,b.name as cgyname from photostyle a 
		left join stylecategory b on b.seq = a.cgySeq
		where 1=1]]></statement>
		<parameter condition="a.forWho =? " kind="2" type="int">forWho</parameter>
		<parameter condition="a.status = ?" kind="2" type="int">status</parameter>
		<parameter condition="a.type = ?" kind="2" type="int">type</parameter>
		<parameter condition="a.cgyseq = ?" kind="2" type="int">cgyseq</parameter>
		<parameter condition="a.name like ?" kind="2" type="LikeString">name</parameter>
	</dao>
	<dao name="qa_photostyle_0">
		<statement><![CDATA[
		select a.*,b.name as cgyname ,
		if(a.type = 1, '封面', '整套') as type_name
		from photostyle a 
		left join stylecategory b on b.seq = a.cgySeq
		where a.forWho = 0]]></statement>
		<orderby>order by a.type,a.width2,a.height2</orderby>
	</dao>

<?xml version="1.0" encoding="UTF-8"?>
<root>
	<dao name="qa_adminuser">
		<statement><![CDATA[select * from adminuser where 1=1]]></statement>
		<statement test="isNotEmpty('keyword')"><![CDATA[
		and (userId like @keyword or userName like @keyword or email like @keyword)
		]]></statement>
		<parameter kind="3" type="LikeString">keyword</parameter>
		<parameter condition="status = ?" kind="2" type="int">status</parameter>
	</dao>
	<dao name="del_adminuser">
		<statement><![CDATA[delete from adminuser where seq = ?]]></statement>
		<parameter type="long">seq</parameter>
	</dao>
	<dao name="qa_seller">
		<statement><![CDATA[select * from seller where 1=1]]></statement>
		<statement test="isNotEmpty('keyword')"><![CDATA[
		and (userId like @keyword or userName like @keyword or email like @keyword)
		]]></statement>
		<parameter kind="3" type="LikeString">keyword</parameter>
		<parameter condition="status = ?" kind="2" type="int">status</parameter>
	</dao>
	<dao name="del_seller">
		<statement><![CDATA[delete from seller where seq = ?]]></statement>
		<parameter type="long">seq</parameter>
	</dao>
</root>

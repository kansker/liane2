<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="system" extends="front-default" namespace="/">
		<action name="admin_manager" class="com.photo.action.Manager">
			<result name="success" type="content">
				<param name="location">/admin/system/manager.jsp</param>
			</result>
		</action>
		<action name="admin_manager_back" class="com.photo.action.Manager" method="back">
			<result name="success" type="content">
				<param name="location">/admin/system/manager.jsp</param>
			</result>
		</action>
		<action name="admin_manager_add" class="com.photo.action.Manager" method="add">
			<result name="success" type="content">
				<param name="location">/admin/system/manager_content.jsp</param>
			</result>
		</action>
		<action name="admin_manager_update" class="com.photo.action.Manager" method="update">
			<result name="success" type="content">
				<param name="location">/admin/system/manager_content.jsp</param>
			</result>
		</action>
		<action name="admin_manager_save" class="com.photo.action.Manager" method="save">
		</action>
		<action name="admin_manager_remove" class="com.photo.action.Manager" method="remove">
		</action>
		<action name="admin_manager_datas" class="com.photo.action.Manager" method="datas" />


		<action name="admin_seller" class="com.photo.action.Seller">
			<result name="success" type="content">
				<param name="location">/admin/system/seller.jsp</param>
			</result>
		</action>
		<action name="admin_seller_back" class="com.photo.action.Seller" method="back">
			<result name="success" type="content">
				<param name="location">/admin/system/seller.jsp</param>
			</result>
		</action>
		<action name="admin_seller_add" class="com.photo.action.Seller" method="add">
			<result name="success" type="content">
				<param name="location">/admin/system/seller_content.jsp</param>
			</result>
		</action>
		<action name="admin_seller_update" class="com.photo.action.Seller" method="update">
			<result name="success" type="content">
				<param name="location">/admin/system/seller_content.jsp</param>
			</result>
		</action>
		<action name="admin_seller_save" class="com.photo.action.Seller" method="save">
		</action>
		<action name="admin_seller_remove" class="com.photo.action.Seller" method="remove">
		</action>
		<action name="admin_seller_datas" class="com.photo.action.Seller" method="datas" />
	</package>
</struts>

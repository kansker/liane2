<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="order" extends="front-default" namespace="/">
		<action name="order" class="com.photo.action.Order">
			<result name="success" type="content">
				<param name="location">/admin/order/order.jsp</param>
			</result>
		</action>
		<action name="order_back" class="com.photo.action.Order" method="back">
			<result name="success" type="content">
				<param name="location">/admin/order/order.jsp</param>
			</result>
		</action>
		<action name="order_datas" class="com.photo.action.Order" method="datas" />
		<action name="order_*" class="com.photo.action.Order" method="{1}" />
	</package>
</struts>

<?xml version="1.0" encoding="big5"?>
<root>
  <dao name="AddOrderLog">
    <statement>insert into order_log(orderNo,GPID,message,createDate) values(?,?,?,?)</statement>
    <parameter type="String">orderNo</parameter>
    <parameter type="String">GPID</parameter>
    <parameter type="String">message</parameter>
    <parameter type="Timestamp">createDate</parameter>
  </dao>
  <dao name="QueryOrderLog">
    <statement>select  * from order_log where orderNo = ? and GPID =?</statement>
    <parameter type="String">orderNo</parameter>
    <parameter type="String">GPID</parameter>
  </dao>
</root>
